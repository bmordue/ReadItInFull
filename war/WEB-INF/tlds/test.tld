<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>

	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>testing</short-name>
	<uri>http://www.tomcat-demo.com/testing</uri>
	<description>This is a demonstration tag library</description>

	<tag>
		<name>twauth</name>
		<tag-class>me.benmordue.gaetest.TwAuth</tag-class>
		<body-content>empty</body-content>
		<description>This tag is for authenticating with Twitter.</description>
	</tag>

	<tag>
		<name>listattr</name>
		<tag-class>me.benmordue.gaetest.ListAttributes</tag-class>
		<body-content>empty</body-content>
		<description>This tag is for listing request and session attributes.</description>
	</tag>

	<tag>
		<name>listfavourites</name>
		<tag-class>me.benmordue.gaetest.ListFavouritesTag</tag-class>
		<body-content>empty</body-content>
		<description>This tag is for listing a user's favorited tweets.</description>
	</tag>


</taglib>
